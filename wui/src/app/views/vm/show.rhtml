<% for column in Vm.content_columns %>
<p>
  <b><%= column.human_name %>:</b> <%=h @vm.send(column.name) %>
</p>
<% end %>

<%= link_to 'Edit', :action => 'edit', :id => @vm %> |
<% if @vm.user -%>
  <%= link_to 'Back', :controller => 'quota', :action => 'show', :id => @vm.user.quota %>
<% else -%>
  <%= link_to 'Back', :action => 'list' %>
<% end -%>
