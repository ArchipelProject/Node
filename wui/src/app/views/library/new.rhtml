<div id="left">
<h2><%= _("New VM Library") %></h2>

<% form_tag :action => 'create' do %>
  <%= render :partial => 'form' %>
  <%= submit_tag "Create" %>
<% end %>

</div>

<div id="right">
  <h2>Actions</h2>
  <ul class="actions">
    <li class="action"><%= link_to "back to #{@vm_library.hardware_pool.name}", { :controller => 'pool', :action => 'show', :id => @vm_library.hardware_pool_id }, { :class => "list" } %></li>
  </ul>
</div>
